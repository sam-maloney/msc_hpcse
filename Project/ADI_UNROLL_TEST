#!/bin/bash -l
#SBATCH --job-name=ADI_UNROLL_TEST
#SBATCH --mail-user=maloneys@student.ethz.ch
#SBATCH --time=03:00:00
#SBATCH --nodes=1
#SBATCH --ntasks-per-core=1
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=1
#SBATCH --partition=normal
#SBATCH --constraint=gpu

export OMP_NUM_THREADS=$SLURM_CPUS_PER_TASK

srun ./ADI_AVX_original8.exe 1 98 0.0000005
srun ./ADI_scalar_original8.exe 1 98 0.0000005
srun ./ADI_scalar_transposed8.exe 1 98 0.0000005
srun ./ADI_AVX_original12.exe 1 98 0.0000005
srun ./ADI_scalar_original12.exe 1 98 0.0000005
srun ./ADI_scalar_transposed12.exe 1 98 0.0000005
